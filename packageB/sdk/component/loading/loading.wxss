.loading-content {
    text-align: center;
    width: 85%;
    transform: translate3d(-50%,0,0);
    position: absolute;
    left: 50%;
    top: 20%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.loading-img {
    width: 230px;
    height: 85px;
}
.loading-text {
    font-size: 18rpx;
}
.loading-progress {
    width: 100%;
}
